# Maintainer: aserdev <you@example.com>

pkgname=aserdev-os-skel
pkgver=$(date +%s)
pkgrel=1
pkgdesc="Default /etc/skel setup for aserdev OS"
arch=('any')
url="https://github.com/aserdevyt/aserdev-dotfiles"
license=('custom')
depends=('unzip' 'aserdev-os-all')
source=("https://github.com/aserdevyt/aserdev-os/raw/refs/heads/main/skel.zip")
md5sums=('SKIP')



package() {
    mkdir -p "$pkgdir/etc/skel"
    unzip -o "$srcdir/skel.zip" -d "$pkgdir/etc/skel"
}
